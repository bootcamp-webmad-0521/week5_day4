<div class="container">

    <h1>Galería de películas</h1>

    <hr>

    <div class="row images-inline-gallery">

        {{#each movies}}

        <div class="col-md-2">
            <figure>
                <img src="{{path}}" alt="{{description}}">
            </figure>
        </div>

        {{/each}}

    </div>

</div>